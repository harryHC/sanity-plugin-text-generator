{
  "name": "sanity-plugin-text-generator",
  "version": "1.0.0",
  "description": "Text generation plugin for Sanity using MediaPipe and Gemma 3",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/harryHC/sanity-plugin-text-generator#readme",
  "bugs": {
    "url": "https://github.com/harryHC/sanity-plugin-text-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/harryHC/sanity-plugin-text-generator.git"
  },
  "license": "MIT",
  "author": "Harry Theo <harry@techcenter.cy>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@mediapipe/tasks-genai": "^0.10.22",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2.15.8"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^7.1.1",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^19.0.12",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.80.1",
    "styled-components": "^6.1.16",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "tsc": false
    }
  }
}
